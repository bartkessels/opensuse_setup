[Tool/XeLaTeX/PDF]
autoRun=yes
checkForRoot=yes
class=LaTeX
command=xelatex
from=
jumpToFirstError=yes
options=-interaction=nonstopmode '%source'
to=pdf
type=Process

[Tool/XeLaTeX/PDF Draft]
autoRun=yes
checkForRoot=yes
class=LaTeX
command=xelatex
from=
jumpToFirstError=yes
options=-draftmode -interaction=nonstopmode '%source'
to=pdf
type=Process

